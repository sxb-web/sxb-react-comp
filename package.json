{
  "name": "sxb-react-comp",
  "version": "1.0.8",
  "description": "Sxb react components.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --mode development",
    "site:deploy": "webpack --config webpack.config.js --mode production && gh-pages -d site",
    "package:build": "rimraf lib es && gulp"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "packages/**/style/*",
    "src/**/*.scss",
    "src/**/*.css",
    "*.css",
    "*.scss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxb-web/sxb-react-comp.git"
  },
  "keywords": [
    "frontend",
    "react",
    "component"
  ],
  "author": "sxb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sxb-web/sxb-react-comp/issues"
  },
  "homepage": "https://github.com/sxb-web/sxb-react-comp#readme",
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "browserslist": [
    "Android >= 4.0",
    "iOS >= 8"
  ],
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "@better-scroll/core": "^2.1.1",
    "@better-scroll/slide": "^2.1.1",
    "@better-scroll/wheel": "^2.1.1",
    "react-lazyload": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.14.0",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.2",
    "gh-pages": "^3.1.0",
    "github-markdown-css": "^4.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "html-webpack-plugin": "^4.4.1",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-markdown": "^4.3.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "through2": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "files": [
    "es",
    "lib"
  ]
