
{
  "presets": ["@babel/env", "@babel/react"],
  "plugins": ["@babel/plugin-transform-runtime", "react-hot-loader/babel"],
  "env": {
    "es": {
      "presets": [[
        "@babel/env",
        {
          "modules": false
        }
      ]],
      "plugins": [[
        "@babel/plugin-transform-runtime",
        {
          "useESModules": true
        }
      ]]
    }
  }
}